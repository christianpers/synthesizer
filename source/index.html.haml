#envelopeControlArea.container
	%form{:id =>"env-form"}
		%label{:for => "attack"} Attack
		%input.attack{:id =>"attack",:type => "number",:min => "0", :max => "0.1", :step => "0.001", :value => "0.001"}
		%label{:for => "decay"} Decay
		%input.decay{:id =>"decay",:type => "number",:min => "0", :max => "2", :step => "0.1", :value => "0.5"}
		%label{:for => "sustain"} Sustain
		%input.sustain{:id =>"sustain",:type => "number",:min => "0", :max => "4", :step => "0.1", :value => "0.6"}
		%label{:for => "release"} Release
		%input.release{:id =>"release", :type => "number",:min => "0", :max => "4", :step => "0.1", :value => "0.3"}

#filterContainer.container
	%form{:id => "filter-form"}
		%label{:for => "lopass"} Lopass Frequency
		%input.attack{:id =>"lopass",:type => "number",:min => "20", :max => "4000", :step => "10", :value => "440"}
		%label{:for => "hipass"} Hipass Frequency
		%input.attack{:id =>"hipass",:type => "number",:min => "20", :max => "4000", :step => "10", :value => "440"}
	


.oscContainer.container
	#triangleTypeOsc1
		%a.wave-type.active{:href => "#", :data_wave_type => "0"} Sine
		%a.wave-type{:href => "#", :data_wave_type => "1"} Square
		%a.wave-type{:href => "#", :data_wave_type => "2"} Sawtooth
		%a.wave-type{:href => "#", :data_wave_type => "3"} Triangle
		%form#osc1gainform.formelement
			%label{:for => "osc1gain"} Gain
			%input{:id => "osc1gain", :type => "number",:min => "0", :max => "1", :step => "0.1", :value => ".5"}

	.osc2Container
		#triangleTypeOsc2
			%a.wave-type.active{:href => "#", :data_wave_type => "0"} Sine
			%a.wave-type{:href => "#", :data_wave_type => "1"} Square
			%a.wave-type{:href => "#", :data_wave_type => "2"} Sawtooth
			%a.wave-type{:href => "#", :data_wave_type => "3"} Triangle
		%form#detune_form.formelement
			%label{:for => "detune"} Detune
			%input{:id => "detune", :type => "number",:min => "-400", :max => "400", :step => "5", :value => "0"}
		%form#osc2gainform.formelement
			%label{:for => "osc2gain"} Gain
			%input{:id => "osc2gain", :type => "number",:min => "0", :max => "1", :step => "0.1", :value => ".5"}

		


#freqContainer.container
	%h4.title Octave
	%p.octave#octVal 4

%form#endGain.formelement
	%label{:for => "endgain"} Master Gain
	%input{:id => "endgain", :type => "number",:min => "0", :max => "1", :step => "0.01", :value => ".5"}



